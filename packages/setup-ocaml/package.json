{
  "name": "@ocaml/setup-ocaml",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:main": "esbuild --bundle --format=cjs --outfile=../../dist/index.cjs --platform=node --target=node24  src/index.ts",
    "build:post": "esbuild --bundle --format=cjs --outfile=../../dist/post/index.cjs --platform=node --target=node24 src/post.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@actions/cache": "4.1.0",
    "@actions/core": "1.11.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.1",
    "@actions/glob": "0.5.0",
    "@actions/http-client": "3.0.0",
    "@actions/io": "2.0.0",
    "@actions/tool-cache": "2.0.2",
    "@octokit/plugin-retry": "8.0.3",
    "cheerio": "1.1.2",
    "exponential-backoff": "3.1.3",
    "semver": "7.7.3",
    "systeminformation": "5.27.11",
    "yaml": "2.8.2"
  },
  "devDependencies": {
    "@tsconfig/node24": "24.0.3",
    "@tsconfig/strictest": "2.0.8",
    "@types/node": "24.10.1",
    "@types/semver": "7.7.1",
    "esbuild": "0.27.0",
    "typescript": "5.9.3"
  }
}
