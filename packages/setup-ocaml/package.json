{
  "name": "@ocaml/setup-ocaml",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:main": "esbuild --bundle --format=cjs --outfile=../../dist/index.cjs --platform=node --target=node20  src/index.ts",
    "build:post": "esbuild --bundle --format=cjs --outfile=../../dist/post/index.cjs --platform=node --target=node20 src/post.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@actions/cache": "4.0.5",
    "@actions/core": "1.11.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.1",
    "@actions/glob": "0.5.0",
    "@actions/http-client": "2.2.3",
    "@actions/io": "1.1.3",
    "@actions/tool-cache": "2.0.2",
    "@octokit/plugin-retry": "8.0.1",
    "cheerio": "1.1.2",
    "exponential-backoff": "3.1.2",
    "semver": "7.7.2",
    "systeminformation": "5.27.7",
    "yaml": "2.8.1"
  },
  "devDependencies": {
    "@tsconfig/node24": "24.0.1",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "24.2.0",
    "@types/semver": "7.7.0",
    "esbuild": "0.25.8",
    "typescript": "5.9.2"
  }
}
